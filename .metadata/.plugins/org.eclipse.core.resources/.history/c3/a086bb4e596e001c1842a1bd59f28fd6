package classe;

public class DataTeste {
	/*
	Instanciar duas datas
	*/
	
	public static void main(String[] args) {
		Data hoje = new Data();
		hoje.dia = 10;
		hoje.mes = 8;
		hoje.ano = 2001; 
		
		Data dia2 = new Data();
		dia2.dia = 22;
		dia2.mes = 11;
		dia2.ano = 2003;
		
		hoje.dataFormatada()
		System.out.printf("Data: %d/%d/%d", dia2.dia, dia2.mes, dia2.ano);
	}
}
