	public static void main(String[] args) {
		
		// if(...) setença ou {}
		// while(...) setença; ou {}
		// for(..;...;...) setença ; ou {}
		// do {} while(...);
		
		Scanner leitor = new Scanner(System.in);

		leitor.close();
	}
}
