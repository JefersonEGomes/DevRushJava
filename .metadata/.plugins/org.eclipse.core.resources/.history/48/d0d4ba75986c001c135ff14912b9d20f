package controle;

public class DesafioIF {
	public static void main(String[] args) {
		double nota = 1.3;
		/* Colocar ";" ponto e virgula na condicional gera um bug
		 onde não  acusa nenhum erro , mas , a sua execução é falha
		 */
		if(nota >= 9.0); { 
			System.out.println("Quadro de Honra");
			System.out.println("Parabéns");
		}
	}
}
