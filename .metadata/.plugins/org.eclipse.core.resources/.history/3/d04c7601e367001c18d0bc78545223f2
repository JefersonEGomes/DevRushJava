package fundamentos;

public class AreaCircunferencia {
	public static void main(String[] args) {
		double raio = 3.4;
		final double PI = 3.14159; // final = const
		
		double area = PI*raio*raio;
		
		System.out.println("Aqui está a sua área: " + area);
		
		raio = 10;
		area = PI*raio*raio;
		System.out.println(area);
	}
}
