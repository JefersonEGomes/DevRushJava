package fundamentos;

public class DesafioTemperatura {
	public static void main(String[] args) {
		double f = 0; // Fahrenheit
		final double sub = 32; // Subtração da formula
		final double mult = 5/9.0; // Multiplicação da formula
		double c = ((f - sub) * mult);
		
		f = 20;
		System.out.println(c);
	}
}
