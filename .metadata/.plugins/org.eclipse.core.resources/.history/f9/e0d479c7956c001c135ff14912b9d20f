package controle;

import java.util.Scanner;

public class ifj {	
	public static void main(String[] args) {
		
			Scanner leitor = new Scanner(System.in);
			
			System.out.println("Informe a média: ");
			double media = leitor.nextDouble();
			
			if(media >= 7) {
				System.out.println("Você passou !!!");
				
			}
	}
}
