package controle;

import java.util.Scanner;

public class Exercicio4 {
	public static void main(String[] args) {
		
				
		Scanner leitor = new Scanner(System.in);
		int i = 1;
		int divisor = 0;
		int num = leitor.nextInt();
		
		while( i <= num) {
			if(num %i == 0) {
				divisor += 1;
				i += 1;
			}
		}
			if(divisor == 0) {
				System.out.println(" é primo");
				
			}else {
				System.out.println("n primo");
				
			}
				
			
		
		
		
		
		leitor.close();
	}
}
