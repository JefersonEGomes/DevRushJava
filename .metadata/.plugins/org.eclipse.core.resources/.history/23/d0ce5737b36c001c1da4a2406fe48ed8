package controle;

import java.util.Scanner;

public class While2 {
	public static void main(String[] args) {
		
		Scanner leitor = new Scanner(System.in);
		String input = leitor.next();
		
		while(!input.equalsIgnoreCase("sair")) {
			System.out.println(input);
			input = leitor.next();
			
		}
		
		
		leitor.close();
	}
}
