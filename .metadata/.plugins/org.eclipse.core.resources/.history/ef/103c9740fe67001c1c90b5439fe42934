package fundamentos;

// A String é imutavel
public class TipoStringExercicios {
	public static void main(String[] args) {
		System.out.println("Olá pessoal".charAt(4));
		
		String s = "Boa tarde";
		s = "Bom dia";
		s = s.toUpperCase();
		System.out.println(s.concat("!!!"));
		System.out.println(s + "!!!!");;
		System.out.println();
	}
}
